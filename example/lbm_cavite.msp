do_domain:
  - domain:
     bounds: [[0,0,0],[0.1,0.1,0.1]]
     resolution: 100
     periodic: [true, false, false]
#     periodic: [false, false, false]

set_lbm_parameters:
  - lbm_parameters:
     Fext: [0,0,0]
     nuth: 1e-4
#     nuth: 1e-2

pre_stream_bcs:
  - pre_bounce_back
  - cavity_z_l:
     U: [0.0, 0.1, 0]

post_stream_bcs:
  - post_bounce_back


global:
   simulation_paraview_freq: 25
   simulation_end_iteration: 10000
